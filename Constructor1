package com.ptpl;

public class Employe {
	
	String name;
	int id;
	
	Employe(String name, int id){
		this.id=id;
		this.name=name;
	}
	
	public static void main(String []args) {
		
		Employe e1=new Employe("shailesh",25);
		Employe e2=new Employe("ram",10);
		
		System.out.println("id="+e1.id+" name="+e1.name);
		System.out.println("id="+e2.id+" name"+e2.name);
	}

}
